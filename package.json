{
  "name": "manager-ui",
  "version": "1.0.0",
  "description": "Application for managing Zesty.io content instances",
  "scripts": {
    "test": "cypress run",
    "clean": "rm -rf build/*",
    "build-local": "npm run clean && concurrently --raw \"npm run gulp\" \"cross-env NODE_ENV=local webpack --config src/shell/webpack.config.js\"",
    "build-dev": "npm run clean && concurrently --raw \"npm run gulp\" \"cross-env NODE_ENV=development webpack --config src/shell/webpack.config.js\"",
    "build-stage": "npm run clean && concurrently --raw \"npm run gulp\" \"cross-env NODE_ENV=stage webpack --config src/shell/webpack.config.js\"",
    "build-prod": "npm run clean && concurrently --raw \"npm run gulp\" \"cross-env NODE_ENV=production webpack --config src/shell/webpack.config.js\"",
    "gulp": "cd src/apps/media; ./node_modules/.bin/gulp",
    "serve": "cross-env serve build/ -l 9000 --single",
    "start": "npm run clean && concurrently --raw \"npm run gulp\" \"cross-env NODE_ENV=development webpack serve --progress --config src/shell/webpack.config.js\"",
    "release:stage": "zutil deploy stage .",
    "release:prod": "npm test && zutil deploy prod .",
    "cleanup": "echo 'All done'."
  },
  "author": "Zesty.io<developers@zesty.io>",
  "contributors": [
    "Stuart Runyan<stuart@zesty.io>",
    "Joshua Hanson<joshua.hanson@zesty.io>",
