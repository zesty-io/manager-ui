{
  "name": "manager-ui",
  "version": "1.0.1",
  "description": "Application for managing a Zesty.io content instance",
  "scripts": {
    "cleanup": "cross-env echo 'FINISHED'",
    "build-local": "npm run task:clean && npm run task:gulp && npm run task:webpack -- --env.NODE_ENV=local",
    "build-dev": "npm run task:clean && npm run task:gulp && npm run task:webpack -- --env.NODE_ENV=development",
    "build-stage": "npm run task:clean && npm run task:gulp && npm run task:webpack -- --env.NODE_ENV=stage",
    "build-prod": "npm run task:clean && npm run task:gulp && npm run task:webpack -- --env.NODE_ENV=production",
    "preinstall": "cd ./src/apps/media && npm install",
    "release:stage": "zutil deploy stage .",
    "release:prod": "npm test && zutil deploy prod .",
    "serve:static": "serve build/ -l 9000 --single",
    "serve:webpack": "webpack-dev-server --display=minimal --progress --config src/shell/webpack.config.js",
    "serve:webpack:ssl": "npm run serve:webpack:ssl -- --https --cert ./etc/ssl/_.manager.dev.zesty.io.crt --key ./etc/ssl/_.manager.dev.zesty.io.key",
    "start": "npm run serve:webpack -- --env.NODE_ENV=development",
    "task:clean": "rimraf build",
    "task:gulp": "cross-env ./src/apps/media/node_modules/.bin/gulp --gulpfile=./src/apps/media/gulpfile.js",
    "task:webpack": "webpack --progress --config src/shell/webpack.config.js",
    "test": "cypress run"
  },
  "author": "Zesty.io<developers@zesty.io>",
  "contributors": [
