{
  "name": "manager-ui",
  "version": "1.0.0",
  "description": "Application for managing Zesty.io content instances",
  "scripts": {
    "test": "cypress run",
    "build-dev": "cross-env NODE_ENV=development node bin/build",
    "build-stage": "cross-env NODE_ENV=stage node bin/build",
    "build-prod": "cross-env NODE_ENV=production node bin/build",
    "start": "concurrently --raw \"node bin/start\" \"npm run serve\"",
    "serve": "cross-env serve build/ -l 9000 --single",
    "release:stage": "zutil deploy stage .",
    "release:prod": "npm test && zutil deploy prod .",
    "cleanup": "echo 'All done'."
  },
  "author": "Zesty.io<developers@zesty.io>",
  "contributors": [
    "Stuart Runyan<stuart@zesty.io>"
  ],
