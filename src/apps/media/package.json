  "dependencies": {
    "concurrently": "^5.2.0",
    "riot": "2.0.15"
  },
  "devDependencies": {
    "babel": "5.8.38",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "7.0.2",
    "gulp": "3.8.11",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.0",
    "gulp-concat": "2.5.2",
    "gulp-jshint": "1.10.0",
    "gulp-less": "3.0.3",
    "gulp-remember": "0.3.0",
    "gulp-riot": "0.3.0"
  }
