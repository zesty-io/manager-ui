{
  "name": "media",
  "version": "1.0.0",
  "description": "Application for managing assets uploaded to an instance",
  "main": "src/index.js",
  "scripts": {
    "test": "cypress run",
    "start": "concurrently \"cross-env ../../../node_modules/.bin/webpack --display=errors-only --progress --watch\" \"gulp\"",
